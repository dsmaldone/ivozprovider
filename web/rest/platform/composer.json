{
    "name": "irontec/ivozprovider/rest-api",
    "description": "Multitenant solution for VoIP telephony providers (Rest-API)",
    "authors": [
        {
            "name": "Carlos Cruz",
            "email": "carlos@irontec.com"
        },
        {
            "name": "Ivan Alonso",
            "email": "kaian@irontec.com"
        },
        {
            "name": "Mikel Madariaga",
            "email": "mikel@irontec.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../../../library/vendor/*/*",
            "options": {
                "symlink": true
            }
        },
        {
            "packagist.org": false
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-0": {
            "IvozProvider\\": "../../../library/IvozProvider"
        },
        "psr-4": {
            "": "src/",
            "Ivoz\\": "../../../library/Ivoz",
            "DataFixtures\\": "../../../library/DataFixtures"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "./tests"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "git"
        },
        "platform": {
            "php": "7.0.19"
        }
    },
    "require": {
        "php": ">=7.0.19",
        "api-platform/core": "2.2.*",
        "irontec/ivoz-api-bundle": "^3.1",
        "irontec/ivoz-provider-bundle": "^2.0",
        "xamin/handlebars.php": "^0.10.4",
        "knplabs/knp-snappy": "0.4.3",
        "twig/twig": "^1.0 || ^2.0"
    },
    "require-dev": {
        "ext-pcntl": "*",
        "behat/behat": "^3.1",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3.1",
        "behat/mink-extension": "^2.2",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^2.5",
        "docteurklein/test-double-bundle": "^1.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "phpunit/phpunit": "^6.0",
        "symfony/phpunit-bridge": "4.1.12"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "public",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "vendor/irontec/ivoz-provider-bundle/Resources/config/parameters.yml"
        },
        "branch-alias": null
    }
}
