{
    "name": "irontec/ivozprovider/schema",
    "description": "Multitenant solution for VoIP telephony providers (Migration handler)",
    "authors": [
        {
            "name": "Carlos Cruz",
            "email": "carlos@irontec.com"
        },
        {
            "name": "Ivan Alonso",
            "email": "kaian@irontec.com"
        },
        {
            "name": "Mikel Madariaga",
            "email": "mikel@irontec.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../library/vendor/*/*",
            "options": {
                "symlink": true
            }
        },
        {
            "packagist.org": false
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "": "src/",
            "DataFixtures\\": "../library/DataFixtures",
            "Ivoz\\": "../library/Ivoz"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "./tests"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "git"
        },
        "platform": {
            "php": "7.0.19"
        }
    },
    "require": {
        "php": ">=7.0.19",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "gesdinet/jwt-refresh-token-bundle": "^0.6.2",
        "graze/guzzle-jsonrpc": "3.2.*",
        "guzzlehttp/guzzle": "^6.3",
        "irontec/ivoz-provider-bundle": "^2.0",
        "irontec/ivoz-dev-tools": "^4.0"
    },
    "require-dev": {
        "docteurklein/test-double-bundle": "^1.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "phpunit/phpunit": "^6.0",
        "sensio/generator-bundle": "^3.0",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/phpunit-bridge": "4.1.12"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "vendor/irontec/ivoz-provider-bundle/Resources/config/parameters.yml"
        },
        "branch-alias": null
    }
}
